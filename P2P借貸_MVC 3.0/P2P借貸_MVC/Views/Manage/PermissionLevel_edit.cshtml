@model P2P借貸_MVC.Models.Definition

@{
    ViewBag.Title = "PermissionLevel_edit";
}

<h2>PermissionLevel_edit</h2>
<hr />

@using (Html.BeginForm("PermissionLevel_Edit", "Manage", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{
    <input type="hidden" id="BankCode" name="BankCode" value=@Model.PermissionLevel />

    <div class="form-group">
        @Html.LabelFor(s => s.PermissionLevel, new { @class = "col-sm-2 control-label" })
        <div class="col-sm-10">
            @Html.EditorFor(s => s.PermissionLevel, new { htmlAttributes = new { @class = "form-control", placeholder = "Permission Level", disabled = "disabled" } })


        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(s => s.PermissionCall, new { @class = "col-sm-2 control-label" })
        <div class="col-sm-10">
            @Html.EditorFor(s => s.PermissionCall, new { htmlAttributes = new { @class = "form-control", placeholder = "Permission Call" } })


        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-5 col-sm-10">
            <button type="submit" class="btn btn-default">修改</button>
        </div>
    </div>
}

@Html.ActionLink("回到上一頁", "Manage", "Manage")

